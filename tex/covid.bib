	journal = {medRxiv},
	publisher = {Cold Spring Harbor Laboratory Press},
	title = {Face masks prevent transmission of respiratory diseases: a meta-analysis of randomized controlled trials},
	url = {https://www.medrxiv.org/content/early/2020/08/04/2020.07.31.20166116},
	year = {2020},
